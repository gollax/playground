package org.gollax.matrixtraversal;

import java.io.Console;

public class MatrixTester {

	public static void main(String[] args) {
		int[][] values = new int[][]{{1,2},{3,4}};
		
//		Matrix m = new Matrix(values, 2, 2);
//		//Should print 1 2 4 3
//		MatrixTraversal.traverse(m);
		
		values = new int[][] {
				{1,2,3},
				{4,5,6},
				{7,8,9}
		};
		//Should print 1,2,3,6,9,8,7,4,5
		System.out.println("----------");
		Matrix m = new Matrix(values,3,3);
		MatrixTraversal.traverse(m);
		System.out.println("----------");
		
		values = new int[][]{
				{1,2,3,4},
				{5,6,7,8}
				
		};
		//Should print 1,2,3,4,8,7,6,5
		MatrixTraversal.traverse(new Matrix(values,2,4));
		
	}

}
