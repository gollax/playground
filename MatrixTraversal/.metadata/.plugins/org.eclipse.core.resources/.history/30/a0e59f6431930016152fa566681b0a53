package org.gollax.matrixtraversal;

public class MatrixTester {

	public static void main(String[] args) {
		int[][] values = new int[][]{{1,2},{3,4}};
		
		Matrix m = new Matrix(values, 2, 2);
		//Should print 1 2 4 3
		MatrixTraversal.traverse(m);
		
		values = new int[][] {
				{1,2,3},
				{4,5,6},
				{7,8,9}
		};
		MatrixTraversal.traverse(m);
	}

}
